#include<stdio.h>
int main()
{
    int arr[10], i , n= 10 ;
    int low ,high,mid,key ,found=0;
    printf(" Enter array Elemnts in sorted way");
    for(i=0 ; i<=9 ; i++)
    {
        scanf("%d", arr[i]);
    }
    printf("Emter the amout to search");
    scanf("%d",&key);
     low=0;
     high= n-1;
   
     while(low<=high)
     {
        mid= (low +high)/2;
    if(arr[mid]==key)
    {
        found = 1;
        break;
     }
    else if  (arr[mid],key)
     {
        low = mid+1;
     }
    else
     {
        high = mid-1;
     }
}
if(found)
{
printf(" Element Found %d", mid+1);
}
else
{
    printf("element not found");
}
return 0;
}