#include<stdio.h>
intmain()
{
    float m1,m2,m3,m4,m5,sum,percentage;
    printf(" Enter the marks for 5 subjects:\n");
    scanf("%f%f%f%f%F",&m,&m2,&m3,&m4,&m5);

    sum= m1+m2+m3+m4+m5 ;
    percentage = (sum*100)/500;
    printf("Total Marks = %.2f\n", sum);
    printf("Percentage = %.2f\n", percentage);
    return 0;
}